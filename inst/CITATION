citHeader("To cite package 'AICcmodavg' in publications use:")
desc <- packageDescription("AICcmodavg")
year <- unlist(strsplit(desc$Date, split="-"))[1]
vers <- paste("R package version", desc$Version)
citEntry(entry="Manual",
	title = "AICcmodavg: Model selection and multimodel inference based on (Q)AIC(c)",
        author = personList(as.person("Marc J. Mazerolle")),
	       year = year,
               note = vers,
	       url = "http://CRAN.R-project.org/package=AICcmodavg",	       
               textVersion =
               paste("Marc J. Mazerolle (", 
	       year, ") AICcmodavg: Model selection and multimodel inference based on (Q)AIC(c). ",
                      vers, ". http://CRAN.R-project.org/package=AICcmodavg.", sep=""))
